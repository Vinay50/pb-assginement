<div class="container">
  <% @projects.each do |project| %>
    <div class="col-md-4">
      <div class="card project-boundry">
        <div class="card-body">
          <h4 class="card-title"><%= link_to "#{project.name}", project_path(project.id) %></h4>
          <h6 class="card-subtitle mb-2 text-muted">Created on:<%= project.created_at %></h6>
          <h6 class="card-subtitle mb-2 text-muted"><%= project.description %></h6>
          <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
          <h4> <%= link_to "Veiw More", project_path(project.id) %></h4>
        </div>
      </div>
    </div>
  <% end %>
</div>
